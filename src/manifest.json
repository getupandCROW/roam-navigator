{
  "name": "Roam Navigator",
  "author": "Michael Sloan",
  "description": "Adds keybindings for navigating Roam",
  "version": "1",
  "homepage_url": "https://github.com/mgsloan/roam-navigator",
  "content_scripts": [
    {
      "matches": ["*://roamresearch.com/*"],
      "js": ["inject.js"],
      "all_frames": true
    }
  ],
  "externally_connectable": {
    "matches": ["*://roamresearch.com/*"]
  },
  "web_accessible_resources": [
    "roam-navigator.js"
  ],
  "browser_action": {
    "default_popup": "options-popup.html"
  },
  "permissions": [
    "storage"
  ],
  "manifest_version": 2
}
